{
    "name": "bobdenotter/tweet-embed",
    "description": "A simple extension to embed tweets from Twitter in your website.",
    "type": "bolt-extension",
    "keywords": [ "tweet", "twitter", "embed" ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "replace": {
        "bobdenotter/bolt-tweet-embed": "*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bob den Otter",
            "email": "bob@twokings.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Bobdenotter\\TweetEmbed\\": "src"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\Bobdenotter\\TweetEmbed\\TweetEmbedExtension"
    }
}
